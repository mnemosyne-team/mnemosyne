{
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "DISABLE_COLLECTSTATIC": {
      "required": true
    },
    "DEFAULT_FROM_EMAIL": {
      "required": true
    },
    "EMAIL_HOST": {
      "required": true
    },
    "EMAIL_HOST_PASSWORD": {
      "required": true
    },
    "EMAIL_HOST_USER": {
      "required": true
    },
    "EMAIL_PORT": {
      "required": true
    },
    "OXFORD_API_URL": {
      "required": true
    },
    "OXFORD_APP_ID": {
      "required": true
    },
    "OXFORD_APP_KEY": {
      "required": true
    },
    "MICROSOFT_SPEECH_API_URL": {
      "required": true
    },
    "MICROSOFT_SPEECH_API_KEY": {
      "required": true
    },
    "AWS_ACCESS_KEY_ID": {
      "required": true
    },
    "AWS_SECRET_ACCESS_KEY": {
      "required": true
    },
    "AWS_STORAGE_BUCKET_NAME": {
      "required": true
    },
    "CLOUDCUBE_URL": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "mnemosyne",
  "scripts": {
  },
  "stack": "heroku-18"
}
